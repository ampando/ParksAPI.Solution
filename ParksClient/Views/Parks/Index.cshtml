@{
    ViewData["Title"] = "Home Page";
}

<h1>New Park:</h1>

<form action="/parks" method="post">
  <label for="name">Park's Name</label>
  <input id="name" name="name" type="text">
  <label for="state">Park's State</label>
  <input id="state" name="state" type="text">
  <label for="type">Park Type</label>
  <input id="type" name="type" type="text">
  <button type="submit">Go!</button>
</form>

<h1>All Parks:</h1>

<ol>
@foreach (Park park in Model)
{
    <ul>
        <li>ID: @park.ParkId</li>
        <li>Name: @park.Name</li>
        <li>State: @park.State</li>
        <li>Type: @park.Type</li>
    </ul>
    <a href="/Parks/Details/@(park.ParkId)"><p>View Details</p></a>
    <br>
}
</ol>